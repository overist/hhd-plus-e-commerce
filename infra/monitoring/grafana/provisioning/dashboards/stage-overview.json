{
  "uid": "stage-overview",
  "title": "Stage Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "type": "text",
      "title": "NestJS",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "options": {
        "content": "**NestJS metrics**",
        "mode": "markdown"
      }
    },
    {
      "type": "timeseries",
      "title": "NestJS CPU % (Otel)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "avg by (container_name) (otel_container_cpu_utilization_ratio{container_name=~\".*app.*\"})",
          "legendFormat": "cpu%"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "NestJS Memory % (vs 512MB limit)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 1
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "(avg by (container_name) (otel_container_memory_percent_ratio{container_name=~\".*app.*\"}) * 4)",
          "legendFormat": "memory%"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "NestJS Total RPS (req/s)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 1
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(otel_http_server_requests_total[1m]))",
          "legendFormat": "total_rps",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "hidden"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "NestJS p95 latency (s)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 1
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(otel_http_server_duration_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "hidden"
        }
      }
    },
    {
      "type": "text",
      "title": "MySQL",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 24
      },
      "options": {
        "content": "**MySQL metrics**",
        "mode": "markdown"
      }
    },
    {
      "type": "timeseries",
      "title": "MySQL CPU % (Otel)",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 0,
        "y": 25
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "rate(otel_container_cpu_usage_nanoseconds_total{container_name=~\".*scale-mysql.*\"}[1m]) / 1000000000 * 100",
          "legendFormat": "cpu%"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "MySQL Memory % (Otel)",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 25
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "otel_container_memory_percent_ratio{container_name=~\".*scale-mysql.*\"}",
          "legendFormat": "resident"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "MySQL threads connected",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 16,
        "y": 25
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "mysql_global_status_threads_connected",
          "legendFormat": "threads_connected",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "hidden"
        }
      }
    },
    {
      "type": "text",
      "title": "Redis",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 30
      },
      "options": {
        "content": "**Redis metrics**",
        "mode": "markdown"
      }
    },
    {
      "type": "timeseries",
      "title": "Redis CPU % (Otel)",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 0,
        "y": 31
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "rate(otel_container_cpu_usage_nanoseconds_total{container_name=~\".*scale-redis.*\"}[1m]) / 1000000000 * 100",
          "legendFormat": "process_cpu_rate%"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Redis memory used",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 31
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "redis_memory_used_bytes",
          "legendFormat": "used",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "hidden"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Redis connected clients",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 16,
        "y": 35
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "redis_connected_clients",
          "legendFormat": "connected",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      }
    }
  ],
  "style": "dark",
  "tags": [
    "stage"
  ],
  "editable": false
}